{
  "name": "scaffold",
  "version": "4.0.2",
  "description": "",
  "scripts": {
    "dev": "set NODE_ENV=development&& webpack-dev-server --config webpack.config.js --port 80",
    "build": "set NODE_ENV=production&& webpack -p --config webpack.config.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "zlz",
  "license": "MIT",
  "jest": {
    "testMatch": [
      "**/tests/acts/*.js?(x)"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "html",
      "text-summary"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "containers": "<rootDir>/src/containers/",
      "actions": "<rootDir>/src/actions/",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "js",
      "scss"
    ]
  },
  "devDependencies": {
    "antd": "^3.4.1",
    "autoprefixer": "^8.2.0",
    "axios": "^0.18.0",
    "axios-mock-adapter": "^1.15.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-import": "^1.7.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.22.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "mini-css-extract-plugin": "^0.4.2",
    "node-sass": "^4.8.3",
    "open-browser-webpack-plugin": "^0.0.5",
    "postcss-loader": "^2.1.3",
    "redux-mock-store": "^1.5.3",
    "sass-loader": "^7.0.0",
    "style-loader": "^0.20.3",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  },
  "dependencies": {
    "immutable": "^3.8.2",
    "prop-types": "^15.6.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-immutable": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  }
}
